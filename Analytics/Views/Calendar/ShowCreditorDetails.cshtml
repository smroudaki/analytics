@model List<CalendarEventDetailsViewModel>

<div id="Details-tabel">

    @if (Model.Count <= 0)
    {
        <div class="row userprofile-detiles-item">
            <div class="col-12 text-center" style="padding: 5px 0px 15px 0px">
                جزییاتی یافت نشد
            </div>
        </div>
    }
    else
    {
        foreach (var item in Model)
        {
            <div class="row userprofile-detiles-item">
                <div class="col-md-3 userprofile-detiles-col-3-main">
                    <div class="panel userprofile-detiles-col-3 ">
                        <span class="userprofile-detiles-text">
                            @item.TrasnactionType
                        </span>
                    </div>
                </div>
                <div class="col-md-9">
                    <div class="panel userprofile-detiles-col-9">
                        <span class="userprofile-detiles-text">
                            @item.CostSum
                        </span>
                    </div>
                </div>
            </div>
        }
    }

</div>